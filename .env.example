# Telegram Bot Credentials
# Obten estos valores desde https://my.telegram.org para API_ID y API_HASH
# Obten el bot token desde @BotFather en Telegram

API_ID=tu_api_id_aqui
API_HASH=tu_api_hash_aqui
API_TOKEN=tu_bot_token_aqui

# NextAuth.js Secret Key
# Genera una clave segura con: openssl rand -base64 32
# Esta clave es necesaria para el sistema de autenticación del dashboard
NEXTAUTH_SECRET=genera_una_clave_secreta_aqui

# Opcional: Configuración de logs
# LOG_LEVEL=INFO
# LOG_DIR=logs
# CONSOLE_LOG_LEVEL=INFO
# FILE_LOG_LEVEL=DEBUG
